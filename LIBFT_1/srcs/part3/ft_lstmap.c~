#include "libft.h"

t_list *ft_lstmap(t_list *lst, t_list *(*f)(t_list *elem))
{
	t_list ret_lst;

	if (lst && f)
	{
		while (lst)
		{
			ret_lst = f(lst);
			lst = lst->next;
			ret_lst=ret_lst->next;
		}
	}
}
